<% if notice %>
  <p class="alert alert-info">
    <a href="#" data-dismiss="alert" class="close" aria-label="Close">&times;</a>
    <%= notice %>
  </p>
<% end %>
<% if alert %>
  <p class="alert alert-danger">
    <a href="#" data-dismiss="alert" class="close" aria-label="Close">&times;</a>
    <%= alert %>
  </p>
<% end %>
